<%= @vidurl[:url] %>

<div class="player">
  <div class="player-title"><h2><%= @vidurl['title'] %></h2></div>
  
  <div class="player-box">
    <iframe width="640" height="480" src="http://www.youtube.com/embed/<%= @vidurl['url'] %>?rel=0"frameborder="0" allowfullscreen></iframe> 
  </div>
  
  <div class="player-dscr"><%= @vidurl['dscr'] %></div>

  <a class="edit" href="/videos/<%= @vidurl['id'] %>/edit">Edit Video</a>
</div>